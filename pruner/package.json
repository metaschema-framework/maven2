  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.17",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.4",
    "js-yaml": "^4.1.0"
  }
}
